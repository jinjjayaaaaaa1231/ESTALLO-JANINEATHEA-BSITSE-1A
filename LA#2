print("comparison Operator Functions")
print("==========================")

x= int(input("Type your desired number: "))
y= int(input("Type your desired number: "))

print(f"",x ,"==", y ,"= ",  x==y)
print(f"",x ,"!=", y ,"= ", x!=y)
print(f"",x ,">", y ,"= ",  x>y)
print(f"",x ,"<", y ,"= ", x<y)
print(f"",x ,">=", y ,"= ", x>=y)
print(f"",x ,"<=", y ,"= ",  x<=y)
